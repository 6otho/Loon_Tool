[Argument]
JD_UNION_ID             = input,   tag="京东联盟 ID"
JD_POSITION_ID          = input,   tag="京东推广位 ID"
JTT_APPID               = input,   tag="京推推 AppID"
JTT_APPKEY              = input,   tag="京推推 AppKey"
TIMEOUT                 = input,   default=60, tag="请求超时时间（秒）"
THEME_TIME              = input,   tag="暗黑模式时间范围", placeholder="起始小时-结束小时，例如 22-6"
阿里云盘签到            = switch,  true,    tag="阿里云盘签到"
快手果园                = switch,  false,   tag="快手果园"
马克喵签到              = switch,  false,   tag="马克喵签到"
微博超话                = switch,  false,   tag="微博超话"
高德地图打车签到        = switch,  false,   tag="高德地图打车签到"

[Script]
cron "0 7,11,17 * * *"  script-path="https://gist.githubusercontent.com/Sliverkiss/33800a98dcd029ba09f8b6fc6f0f5162/raw/aliyun.js", timeout=${TIMEOUT}, tag="阿里云盘签到", img-url="https://raw.githubusercontent.com/fmz200/wool_scripts/main/icons/apps/AliYunDrive.png", enable=${阿里云盘签到}
cron "30 1,8,12,17 * * *" script-path="https://raw.githubusercontent.com/fmz200/wool_scripts/main/Scripts/kuaishou/ks_fruit.js", timeout=${TIMEOUT}, tag="快手果园", img-url="https://raw.githubusercontent.com/fmz200/wool_scripts/main/icons/apps/kuaishou.png", enable=${快手果园}
cron "22 7 * * *"       script-path="https://raw.githubusercontent.com/fmz200/wool_scripts/main/Scripts/macat/macat_signin.js", timeout=${TIMEOUT}, tag="马克喵签到", img-url="https://raw.githubusercontent.com/fmz200/wool_scripts/main/icons/apps/Macat.png", enable=${马克喵签到}
cron "19 7 * * *"       script-path="https://raw.githubusercontent.com/toulanboy/scripts/master/weibo/weibotalk.js", timeout=${TIMEOUT}, tag="微博超话", img-url="https://raw.githubusercontent.com/fmz200/wool_scripts/main/icons/apps/WeiboTalk.png", enable=${微博超话}
cron "1 0 * * *"        script-path="https://raw.githubusercontent.com/wf021325/qx/master/task/ampDache.js", timeout=${TIMEOUT}, tag="高德地图打车签到", img-url="https://raw.githubusercontent.com/fmz200/wool_scripts/main/icons/apps/GaodeMap.jpg", enable=${高德地图打车签到}
